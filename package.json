{
  "name": "obsidian-assistant",
  "version": "0.1.0",
  "description": "Obsidian plugin that uses LLM services to chat with an Obsidian vault",
  "main": "main.js",
  "config": {
    "obsidianPluginsDir": "%USERPROFILE%\\Documents\\Obsidian\\.obsidian\\plugins\\assistant"
  },
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "package": "node create-release.js",
    "install": "node install-plugin.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "llm",
    "ai",
    "chat"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "obsidian": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.5.3"
  }
}
